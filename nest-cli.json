{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auth",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "store": {
      "type": "application",
      "root": "apps/store",
      "entryFile": "main",
      "sourceRoot": "apps/store/src",
      "compilerOptions": {
        "tsConfigPath": "apps/store/tsconfig.app.json"
      }
    },
    "headquarter": {
      "type": "application",
      "root": "apps/headquarter",
      "entryFile": "main",
      "sourceRoot": "apps/headquarter/src",
      "compilerOptions": {
        "tsConfigPath": "apps/headquarter/tsconfig.app.json"
      }
    },
    "contact_infos": {
      "type": "application",
      "root": "apps/contact_infos",
      "entryFile": "main",
      "sourceRoot": "apps/contact_infos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/contact_infos/tsconfig.app.json"
      }
    },
    "store_categories": {
      "type": "application",
      "root": "apps/store_categories",
      "entryFile": "main",
      "sourceRoot": "apps/store_categories/src",
      "compilerOptions": {
        "tsConfigPath": "apps/store_categories/tsconfig.app.json"
      }
    },
    "picks": {
      "type": "application",
      "root": "apps/picks",
      "entryFile": "main",
      "sourceRoot": "apps/picks/src",
      "compilerOptions": {
        "tsConfigPath": "apps/picks/tsconfig.app.json"
      }
    },
    "offers": {
      "type": "application",
      "root": "apps/offers",
      "entryFile": "main",
      "sourceRoot": "apps/offers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/offers/tsconfig.app.json"
      }
    },
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    }
  }
}